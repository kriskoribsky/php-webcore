{
    "name": "kriskoribsky/webcore",
    "description": "Simple, standalone utility constants, functions and classes for PSR implementations.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "utility",
        "constants",
        "psr-7",
        "psr-15",
        "psr-17",
        "psr-18"
    ],
    "authors": [
        {
            "name": "Kristian Koribsky",
            "email": "kristian.koribsky@gmail.com",
            "homepage": "https://kriskoribsky.me"
        }
    ],
    "homepage": "https://github.com/ksoft-server-php/essentials",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.31",
        "friendsofphp/php-cs-fixer": "^3.19",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.14",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpunit/phpunit": "^10",
        "slam/phpstan-extensions": "^6.0",
        "symplify/monorepo-builder": "11.2.3.72"
    },
    "replace": {
        "ksoft/http": "self.version",
        "ksoft/log": "self.version",
        "ksoft/uri": "self.version"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Ksoft\\Essentials\\Http\\": "src/Http/",
            "Ksoft\\Essentials\\Log\\": "src/Log/",
            "Ksoft\\Essentials\\Uri\\": "src/Uri/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "lock": false,
        "sort-packages": true
    }
}
