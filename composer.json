{
  "name": "kriskoribsky/webcore",
  "description": "Core of the custom personal framework designed for web application development.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kristian Koribsky",
      "email": "kiko.koribsky@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "lock": false,
    "sort-packages": true,
    "platform-check": true
  },
  "scripts": {
    "test": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-text"
  },
  "autoload": {
    "files": [],
    "psr-4": {
      "WebCore\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [],
    "psr-4": {}
  },
  "require-dev": {
    "phpunit/phpunit": "^10"
  },
  "require": {
    "php": "^8.2",
    "psr/log": "^3.0"
  }
}
